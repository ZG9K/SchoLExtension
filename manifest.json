{
    "name": "SchoL Debug Seb App",
    "description": "Random schol changes that make it better maybe?",
    "version": "1.0",
    "manifest_version": 2,
    "host_permissions": [
        "https://learning.stmichaels.vic.edu.au/*",
        "https://learning-dev.stmichaels.vic.edu.au/*"
    ],
    "background": { "scripts": ["hot-reload.js"] },
    "permissions": ["storage"],
    "web_accessible_resources": [
        "compiled.js"
    ],
    "content_scripts": [
        {
            "matches": ["https://learning.stmichaels.vic.edu.au/*"],
            "js": ["extension.js"],
            "run_at": "document_idle"
        }
      ]
  }